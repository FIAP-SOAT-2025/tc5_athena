{
  "title": "Athena API - Métricas HTTP",
  "uid": "athena-api",
  "schemaVersion": 36,
  "version": 3,
  "refresh": "30s",
  "time": { "from": "now-1h", "to": "now" },
  "timezone": "browser",
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "Total Requisições (1h)",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum(increase(http_requests_total{kubernetes_namespace=\"tc5-athena\"}[1h]))",
          "legendFormat": ""
        }
      ],
      "options": {
        "reduceOptions": { "values": false, "calcs": ["lastNotNull"], "fields": "" },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "auto"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0,
          "color": { "mode": "fixed", "fixedColor": "blue" },
          "thresholds": { "mode": "absolute", "steps": [{ "color": "blue", "value": null }] },
          "mappings": []
        }
      },
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 0 }
    },
    {
      "id": 2,
      "type": "stat",
      "title": "Requisições / seg",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{kubernetes_namespace=\"tc5-athena\"}[5m]))",
          "legendFormat": ""
        }
      ],
      "options": {
        "reduceOptions": { "values": false, "calcs": ["lastNotNull"], "fields": "" },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "auto"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "decimals": 2,
          "color": { "mode": "fixed", "fixedColor": "green" },
          "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }] },
          "mappings": []
        }
      },
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 0 }
    },
    {
      "id": 3,
      "type": "stat",
      "title": "Taxa de Erros 5xx",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{kubernetes_namespace=\"tc5-athena\",status_code=~\"5..\"}[5m])) / sum(rate(http_requests_total{kubernetes_namespace=\"tc5-athena\"}[5m])) * 100",
          "legendFormat": ""
        }
      ],
      "options": {
        "reduceOptions": { "values": false, "calcs": ["lastNotNull"], "fields": "" },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "auto"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 2,
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1 },
              { "color": "red", "value": 5 }
            ]
          },
          "mappings": []
        }
      },
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 0 }
    },
    {
      "id": 4,
      "type": "stat",
      "title": "Latência P95",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(http_request_duration_seconds_bucket{kubernetes_namespace=\"tc5-athena\"}[5m])))",
          "legendFormat": ""
        }
      ],
      "options": {
        "reduceOptions": { "values": false, "calcs": ["lastNotNull"], "fields": "" },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "auto"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 3,
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 0.5 },
              { "color": "red", "value": 1 }
            ]
          },
          "mappings": []
        }
      },
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 0 }
    },

    {
      "id": 10,
      "type": "row",
      "title": "Tráfego de Requisições HTTP",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 4 }
    },
    {
      "id": 11,
      "type": "timeseries",
      "title": "Taxa de Requisições por Endpoint",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum by (route, method) (rate(http_requests_total{kubernetes_namespace=\"tc5-athena\"}[5m]))",
          "legendFormat": "{{method}} {{route}}"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "reqps", "min": 0 }
      },
      "options": {
        "tooltip": { "mode": "multi" },
        "legend": { "displayMode": "table", "placement": "bottom" }
      },
      "gridPos": { "h": 9, "w": 12, "x": 0, "y": 5 }
    },
    {
      "id": 12,
      "type": "timeseries",
      "title": "Requisições por Status HTTP",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum by (status_code) (rate(http_requests_total{kubernetes_namespace=\"tc5-athena\"}[5m]))",
          "legendFormat": "HTTP {{status_code}}"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "reqps", "min": 0 },
        "overrides": [
          {
            "matcher": { "id": "byRegexp", "options": "HTTP 5.+" },
            "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byRegexp", "options": "HTTP 4.+" },
            "properties": [{ "id": "color", "value": { "fixedColor": "yellow", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byRegexp", "options": "HTTP 2.+" },
            "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }]
          }
        ]
      },
      "options": {
        "tooltip": { "mode": "multi" },
        "legend": { "displayMode": "table", "placement": "bottom" }
      },
      "gridPos": { "h": 9, "w": 12, "x": 12, "y": 5 }
    },

    {
      "id": 20,
      "type": "row",
      "title": "Tempos de Resposta",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 14 }
    },
    {
      "id": 21,
      "type": "timeseries",
      "title": "Percentis de Latência (P50 / P95 / P99)",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum by (le) (rate(http_request_duration_seconds_bucket{kubernetes_namespace=\"tc5-athena\"}[5m])))",
          "legendFormat": "P50"
        },
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(http_request_duration_seconds_bucket{kubernetes_namespace=\"tc5-athena\"}[5m])))",
          "legendFormat": "P95"
        },
        {
          "expr": "histogram_quantile(0.99, sum by (le) (rate(http_request_duration_seconds_bucket{kubernetes_namespace=\"tc5-athena\"}[5m])))",
          "legendFormat": "P99"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "s", "min": 0 }
      },
      "options": {
        "tooltip": { "mode": "multi" },
        "legend": { "displayMode": "list", "placement": "bottom" }
      },
      "gridPos": { "h": 9, "w": 12, "x": 0, "y": 15 }
    },
    {
      "id": 22,
      "type": "timeseries",
      "title": "Tempo Médio de Resposta por Endpoint",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum by (route, method) (rate(http_request_duration_seconds_sum{kubernetes_namespace=\"tc5-athena\"}[5m])) / sum by (route, method) (rate(http_request_duration_seconds_count{kubernetes_namespace=\"tc5-athena\"}[5m]))",
          "legendFormat": "{{method}} {{route}}"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "s", "min": 0 }
      },
      "options": {
        "tooltip": { "mode": "multi" },
        "legend": { "displayMode": "table", "placement": "bottom" }
      },
      "gridPos": { "h": 9, "w": 12, "x": 12, "y": 15 }
    },

    {
      "id": 30,
      "type": "row",
      "title": "Rastreamento de Erros",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 24 }
    },
    {
      "id": 31,
      "type": "timeseries",
      "title": "Erros HTTP (4xx e 5xx) por Endpoint",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum by (route, method, status_code) (rate(http_requests_total{kubernetes_namespace=\"tc5-athena\",status_code=~\"[45]..\"}[5m]))",
          "legendFormat": "{{method}} {{route}} ({{status_code}})"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "min": 0,
          "color": { "fixedColor": "red", "mode": "fixed" }
        }
      },
      "options": {
        "tooltip": { "mode": "multi" },
        "legend": { "displayMode": "table", "placement": "bottom" }
      },
      "gridPos": { "h": 9, "w": 12, "x": 0, "y": 25 }
    },
    {
      "id": 32,
      "type": "timeseries",
      "title": "Taxa de Erros ao Longo do Tempo (%)",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{kubernetes_namespace=\"tc5-athena\",status_code=~\"5..\"}[5m])) / sum(rate(http_requests_total{kubernetes_namespace=\"tc5-athena\"}[5m])) * 100",
          "legendFormat": "Erro 5xx (%)"
        },
        {
          "expr": "sum(rate(http_requests_total{kubernetes_namespace=\"tc5-athena\",status_code=~\"4..\"}[5m])) / sum(rate(http_requests_total{kubernetes_namespace=\"tc5-athena\"}[5m])) * 100",
          "legendFormat": "Erro 4xx (%)"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "percent", "min": 0 },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Erro 5xx (%)" },
            "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "Erro 4xx (%)" },
            "properties": [{ "id": "color", "value": { "fixedColor": "yellow", "mode": "fixed" } }]
          }
        ]
      },
      "options": {
        "tooltip": { "mode": "multi" },
        "legend": { "displayMode": "list", "placement": "bottom" }
      },
      "gridPos": { "h": 9, "w": 12, "x": 12, "y": 25 }
    },

    {
      "id": 40,
      "type": "row",
      "title": "Métricas de Processo Node.js",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 34 }
    },
    {
      "id": 41,
      "type": "timeseries",
      "title": "Uso de CPU (%)",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rate(process_cpu_seconds_total{kubernetes_namespace=\"tc5-athena\"}[1m]) * 100",
          "legendFormat": "CPU %"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "percent", "min": 0 }
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 35 }
    },
    {
      "id": 42,
      "type": "timeseries",
      "title": "Memória Heap",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "nodejs_heap_size_used_bytes{kubernetes_namespace=\"tc5-athena\"}",
          "legendFormat": "Heap Usada"
        },
        {
          "expr": "nodejs_heap_size_total_bytes{kubernetes_namespace=\"tc5-athena\"}",
          "legendFormat": "Heap Total"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "bytes", "min": 0 }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 35 }
    },
    {
      "id": 43,
      "type": "timeseries",
      "title": "Lag do Event Loop (s)",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "nodejs_eventloop_lag_seconds{kubernetes_namespace=\"tc5-athena\"}",
          "legendFormat": "Event Loop Lag"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "s", "min": 0 }
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 43 }
    },
    {
      "id": 44,
      "type": "timeseries",
      "title": "Handles e Requests Ativos",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "nodejs_active_handles_total{kubernetes_namespace=\"tc5-athena\"}",
          "legendFormat": "Handles Ativos"
        },
        {
          "expr": "nodejs_active_requests_total{kubernetes_namespace=\"tc5-athena\"}",
          "legendFormat": "Requests Ativos"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "short", "min": 0 }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 43 }
    }
  ]
}
